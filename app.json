{
  "expo": {
    "name": "11sur10",
    "slug": "11sur10",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./src/assets/logopnn.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logopnn.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.onzesur10.app",
      "supportsTablet": true,
      "pushNotifications": true,
      "buildNumber": "1",
     "infoPlist": {
  "ITSAppUsesNonExemptEncryption": false,
  "UIBackgroundModes": ["remote-notification"]
}
    },
    "notification": {
      "icon": "./src/assets/logopnn.png",
      "color": "#000000",
      "useNextNotificationsApi": true

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logopnn.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.onzesur10.app",
      "versionCode": 2,
      "permissions": [
    "NOTIFICATIONS",
    "WAKE_LOCK",
    "RECEIVE_BOOT_COMPLETED"
  ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Kanit/Kanit-Black.ttf",
            "./src/assets/fonts/Permanent_Marker/PermanentMarker-Regular.ttf"
          ]
        }
      ],
      "expo-build-properties",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "d459ac30-203a-4346-86f2-2b4cb8813c67"
      }
    }
  }
}
